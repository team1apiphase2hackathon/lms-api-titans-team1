{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Reset Password Response Schema",
  "required": [
    "email",
    "roles",
    "status",
    "token",
    "type",
    "userId"
  ],
  "properties": {
    "email": { 
      "type": "string",
      "format": "email"
    },
    "roles": {
      "type": "array",
      "items": { "type": "string" },
      "minItems": 1
    },
    "status": { "type": "string" },
    "token": { "type": "string" },
    "type": { 
      "type": "string",
      "enum": ["Bearer"] 
    },
    "userId": { "type": "string" }
  }
}